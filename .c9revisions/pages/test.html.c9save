\r\n    margin: 0 auto;\r\n    padding: 25px 25px;\r\n    width: 750px;\r\n    border-bottom: 2px solid #ff280f;\r\n}\r\n\r\n.lastpost{\r\n    border-bottom: none;\r\n}\r\n\r\n.poststamp{\r\n    padding-top: 10px;\r\n    font-size: 1em;\r\n}\r\n\r\n.poststamp p{\r\n    font-size: 1em;\r\n}\r\n\r\n.contentIndent{\r\n    padding: 30px 120px;\r\n}\r\n\r\n#earthImg{\r\n    border-radius: 5px;\r\n}\r\n\r\ndiv.imgBoxLeft{\r\n    float: left;\r\n    padding: 10px;\r\n}\r\n\r\ndiv.imgBoxRight{\r\n    float: right;\r\n}\r\n\r\n/* END BLOGGPOSTS */\r\n\r\n#presentationImg{\r\n    margin: 20px;\r\n    border: 2px solid #4B81BA;\r\n}\r\n\r\n#hobbylist{\r\n    margin-left: 20px;\r\n    margin-bottom: 20px;    \r\n}\r\n\r\n#hobbylist ul li{\r\n    padding: 5px;\r\n}\r\n\r\n#programmingList{\r\n    margin: 0  0 20px 30px;\r\n}\r\n\r\n#programmingList ul li.listHeading{\r\n    list-style-type: circle;\r\n    font-weight: bold;\r\n}\r\n\r\n#programmingList li{\r\n    padding: 5px;\r\n}\r\n\r\n#programmingList li ol{\r\n    margin-left: 35px;\r\n    list-style-type: decimal;\r\n    font-size: 0.8em;\r\n}\r\n\r\n#bookList{\r\n    margin: 0 0 20px 10px;\r\n    font-family: Arial, Helvetiva, sans-serif;\r\n}\r\n\r\n#bookList dt{\r\n    padding-top: 10px;\r\n    font-weight: bold;\r\n}\r\n\r\n.laboration{\r\n    margin-bottom: 30px;\r\n}\r\n\r\ndd{\r\n    margin-left: 20px;\r\n    padding-top: 3px;\r\n}\r\n\r\n#urlList dt{\r\n    padding-top: 10px;\r\n}\r\n\r\n#urlList li{\r\n    padding: 5px;\r\n}\r\n\r\n/* END Lists */\r\n\r\n.top{\r\n    border: 2px solid #184574;\r\n    margin-bottom: 30px;\r\n}\r\n\r\n.bottom{\r\n    border: 2px solid #184574;\r\n}\r\n\r\nlegend{\r\n    margin-left: 20px;\r\n    font-weight: bold;\r\n    font-size: 1.1em;\r\n    font-style: \"arial-black\", Helvetica, sans-serif;\r\n}\r\n\r\n.label{\r\n    display: inline-block;\r\n    width: 200px;\r\n    vertical-align: top;\r\n    text-align: right;\r\n    margin-right: 20px;\r\n}\r\n\r\n.mail{\r\n    height: 20px;\r\n    background: url('pics/sprite.png') no-repeat left center;\r\n    background-position: 0 -287px;\r\n    padding-top: 5px;\r\n}\r\n\r\n.home{\r\n    height: 20px;\r\n    background: url('pics/sprite.png') no-repeat left center;\r\n    background-position: 0 0px;\r\n    padding-top: 5px;\r\n}\r\n\r\n.contact{\r\n    margin-top: 5px;\r\n    height: 20px;\r\n}\r\n\r\nselect{\r\n    border: 1px solid #233D57;\r\n}\r\n\r\nselect:focus{\r\n    border: 1px solid #233D57;\r\n}\r\n\r\n.textfield{\r\n    border: 1px solid #233D57;\r\n}\r\n\r\ninput.textfield{\r\n    padding: 5px;\r\n}\r\n\r\n.textfield:focus{\r\n    border: 1px solid #233D57;\r\n    font-style: Arial, Helvetica sans-serif;\r\n    color: #000;\r\n}\r\n/* END FORM STYLING */\r\n\r\n/* CONTENT END */\r\n\r\n#footer{\r\n    clear: both;\r\n    height: 200px;\r\n    background-color: #fff;\r\n    border-top: 1px solid #ff280f;\r\n}\r\n\r\n#footer p{\r\n    text-align: center;\r\n    margin-top: 90px;\r\n}\r\n"]],"start1":0,"start2":0,"length1":0,"length2":7923}]],"length":7923,"saved":false}
